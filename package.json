{
  "name": "fluidnode",
  "version": "0.0.1",
  "description": "A simple skeleton for a node based Fluid Client",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run tsc && npm run build:test",
    "tsc": "tsc",
    "webpack": "webpack",
    "start": "npm run build && node dist/index.js",
    "start:server": "tinylicious",
    "build:test": "tsc --project ./test/tsconfig.json",
    "test:mocha": "mocha --recursive dist/test -r node_modules/@fluidframework/mocha-test-setup --unhandled-rejections=strict",
    "test": "npm run build:test && npm run test:mocha"
  },
  "keywords": [
    "Fluid",
    "Node"
  ],
  "author": "Marcus Karlbowski",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/express": "^4.17.11",
    "@types/node": "^15.3.1",
    "tinylicious": "^0.4.21640",
    "typescript": "^4.2.4",
    "@fluidframework/local-driver": "^0.40.0-25719",
    "@fluidframework/mocha-test-setup": "^0.40.0-25719",
    "@fluidframework/runtime-utils": "^0.40.0-25719",
    "@fluidframework/server-local-server": "^0.1025.1",
    "@fluidframework/test-utils": "^0.40.0-25719",
    "@types/chai": "^4.2.18",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^8.2.2",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.4.0",
    "ts-node": "^10.0.0"
  },
  "dependencies": {
    "@fluidframework/aqueduct": "^0.40.0-25719",
    "@fluidframework/ordered-collection": "^0.40.0-25719",
    "@fluid-experimental/tinylicious-client": "^0.40.0-25719",
    "@fluid-experimental/fluid-static": "^0.40.0-25719",
    "@fluidframework/map": "^0.40.0-25719",
    "@fluidframework/container-definitions": "^0.39.3",
    "@fluid-experimental/get-container": "^0.40.0-25719",
    "uuid": "^8.3.2",
    "express": "^4.17.1"
  }
}
